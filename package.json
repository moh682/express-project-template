{
  "name": "express-project-template",
  "version": "1.0.0",
  "description": "Express Project Template with Typescript, Eslint and Prettier",
  "main": "src/server.ts",
  "scripts": {
    "start:dev": "ENVIRONMENT=development nodemon -e ts,js --exec ts-node -r tsconfig-paths/register ./src/server.ts",
